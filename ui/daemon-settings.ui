<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkAdjustment" id="adjKeepVersions">
    <property name="lower">1</property>
    <property name="upper">99999</property>
    <property name="value">5</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
  </object>
  <object class="GtkAdjustment" id="adjMaxRecvKbps">
    <property name="upper">99999</property>
    <property name="value">1</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
  </object>
  <object class="GtkAdjustment" id="adjMaxSendKbps">
    <property name="upper">99999</property>
    <property name="value">1</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
  </object>
  <object class="GtkAdjustment" id="adjRescan">
    <property name="lower">1</property>
    <property name="upper">9999</property>
    <property name="value">50</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
  </object>
  <object class="GtkAdjustment" id="adjlocalAnnouncePort">
    <property name="lower">1</property>
    <property name="upper">65535</property>
    <property name="value">21025</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
  </object>
  <object class="GtkDialog" id="editor">
    <property name="modal">1</property>
    <property name="default_width">500</property>
    <child internal-child="content_area">
      <object class="GtkBox" id="dialog-vbox1">
        <property name="orientation">vertical</property>
        <property name="spacing">2</property>
        <child>
          <object class="GtkGrid" id="grid1">
            <property name="vexpand">1</property>
            <property name="margin-start">10</property>
            <property name="margin-end">10</property>
            <property name="column_spacing">3</property>
            <property name="column_homogeneous">1</property>
            <child>
              <object class="GtkLabel" id="label1">
                <property name="label" translatable="1">Sync Protocol Listen Addresses</property>
                <property name="use_underline">1</property>
                <property name="mnemonic_widget">vlistenAddresses</property>
                <property name="xalign">0</property>
                <attributes>
                  <attribute name="weight" value="bold"></attribute>
                </attributes>
                <layout>
                  <property name="column">0</property>
                  <property name="row">0</property>
                  <property name="column-span">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkEntry" id="vlistenAddresses">
                <property name="margin_bottom">10</property>
                <signal name="changed" handler="cb_check_value" swapped="no"/>
                <layout>
                  <property name="column">0</property>
                  <property name="row">1</property>
                  <property name="column-span">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="vlocalAnnounceEnabled">
                <property name="label" translatable="1">Enable Local _Discovery</property>
                <property name="use_underline">1</property>
                <signal name="toggled" handler="ui_value_changed" swapped="no"/>
                <layout>
                  <property name="column">0</property>
                  <property name="row">6</property>
                  <property name="column-span">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="vupnpEnabled">
                <property name="label" translatable="1">Enable _UPnP</property>
                <property name="use_underline">1</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">12</property>
                  <property name="column-span">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="lblvlocalAnnouncePort">
                <property name="label" translatable="1">Local Discovery _Port</property>
                <property name="use_underline">1</property>
                <property name="mnemonic_widget">vlocalAnnouncePort</property>
                <property name="xalign">0</property>
                <attributes>
                  <attribute name="weight" value="bold"></attribute>
                </attributes>
                <layout>
                  <property name="column">0</property>
                  <property name="row">7</property>
                  <property name="column-span">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkSpinButton" id="vmaxSendKbps">
                <property name="margin_bottom">10</property>
                <property name="adjustment">adjMaxSendKbps</property>
                <signal name="output" handler="cb_format_value_kibps_or_no_limit" swapped="no"/>
                <layout>
                  <property name="column">0</property>
                  <property name="row">5</property>
                  <property name="column-span">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="vmaxSendKbpsEnabled">
                <property name="use_underline">1</property>
                <signal name="toggled" handler="ui_value_changed" swapped="no"/>
                <child>
                  <object class="GtkLabel" id="label6">
                    <property name="label" translatable="1">Outgoing _Rate Limit</property>
                    <property name="use_underline">1</property>
                    <property name="mnemonic_widget">vmaxSendKbpsEnabled</property>
                    <attributes>
                      <attribute name="weight" value="bold"></attribute>
                    </attributes>
                  </object>
                </child>
                <layout>
                  <property name="column">0</property>
                  <property name="row">4</property>
                  <property name="column-span">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="vstartBrowser">
                <property name="label" translatable="1">Open browser with _WebUI when daemon is starting</property>
                <property name="margin_bottom">10</property>
                <property name="use_underline">1</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">14</property>
                  <property name="column-span">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="vurAccepted">
                <property name="label" translatable="1">Enable _Anonymous Usage Reporting</property>
                <property name="use_underline">1</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">13</property>
                  <property name="column-span">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="vglobalAnnounceEnabled">
                <property name="label" translatable="1">Enable _Global Discovery</property>
                <property name="use_underline">1</property>
                <signal name="toggled" handler="ui_value_changed" swapped="no"/>
                <layout>
                  <property name="column">0</property>
                  <property name="row">9</property>
                  <property name="column-span">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="lblvglobalAnnounceServers">
                <property name="label" translatable="1">Global Discovery _Servers</property>
                <property name="use_underline">1</property>
                <property name="mnemonic_widget">vglobalAnnounceServers</property>
                <property name="xalign">0</property>
                <attributes>
                  <attribute name="weight" value="bold"></attribute>
                </attributes>
                <layout>
                  <property name="column">0</property>
                  <property name="row">10</property>
                  <property name="column-span">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkEntry" id="vglobalAnnounceServers">
                <property name="margin_bottom">15</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">11</property>
                  <property name="column-span">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkSpinButton" id="vlocalAnnouncePort">
                <property name="margin_bottom">10</property>
                <property name="text">50</property>
                <property name="adjustment">adjlocalAnnouncePort</property>
                <property name="value">50</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">8</property>
                  <property name="column-span">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="vmaxRecvKbpsEnabled">
                <property name="use_underline">1</property>
                <signal name="toggled" handler="ui_value_changed" swapped="no"/>
                <child>
                  <object class="GtkLabel" id="label2">
                    <property name="label" translatable="1">_Incoming Rate Limit</property>
                    <property name="use_underline">1</property>
                    <property name="mnemonic_widget">vmaxRecvKbpsEnabled</property>
                    <attributes>
                      <attribute name="weight" value="bold"></attribute>
                    </attributes>
                  </object>
                </child>
                <layout>
                  <property name="column">0</property>
                  <property name="row">2</property>
                  <property name="column-span">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkSpinButton" id="vmaxRecvKbps">
                <property name="margin_bottom">10</property>
                <property name="adjustment">adjMaxRecvKbps</property>
                <signal name="output" handler="cb_format_value_kibps_or_no_limit" swapped="no"/>
                <layout>
                  <property name="column">0</property>
                  <property name="row">3</property>
                  <property name="column-span">2</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child internal-child="action_area">
      <object class="GtkBox" id="dialog-action_area1">
        <child type="action">
          <object class="GtkButton" id="btClose">
            <property name="label">Cancel</property>
            <property name="receives_default">1</property>
          </object>
        </child>
        <child type="action">
          <object class="GtkButton" id="btSave">
            <property name="label">Save</property>
            <property name="sensitive">0</property>
            <property name="receives_default">1</property>
          </object>
        </child>
      </object>
    </child>
    <action-widgets>
      <action-widget response="close">btClose</action-widget>
      <action-widget response="apply">btSave</action-widget>
    </action-widgets>
  </object>
</interface>
